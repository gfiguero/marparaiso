{% block contact %}
    <section id="contact">
        <div class="container">
            <div class="row text-center">
                <div class="col-lg-12">
                    <h2 class="section-heading">Contacto</h2>
                </div>
                <div class="col-md-12">
                    <p class="text-muted">{{ icon('envelope fa-2x') }}&nbsp;{{ frontpage.frontpageemail }}</p>
                    <p class="text-muted">{{ icon('phone fa-2x') }}&nbsp;{{ frontpage.frontpagephonenumber }}</p>
                    <p class="text-muted">{{ icon('map-marker fa-2x') }}&nbsp;{{ frontpage.frontpageaddress }}</p>
                </div>
            </div>
        </div>
    </section>
{% endblock %}